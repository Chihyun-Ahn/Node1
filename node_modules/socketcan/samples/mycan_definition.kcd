<NetworkDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="Definition.xsd"
 version="0.3">
	<Document name="The Anchi" version="1.00" author="Andy Ahn" company="Kwangwoon University" date="2019-08-02">
	 A CAN description for a simple raspberry pi project.
	 If you have any inquiry, please contact me at anchi15@samwooinc.net
	</Document> 
	<Node id="1" name="Motor ACME"/>
    
	<Bus name="Sensor">
		<Message id="0x001" name="Temperature">
			<Signal name="Value" offset="0"/>
		</Message>
		
		<Message id="0x002" name="Humidity">
	    	<Signal name="Value" offset="0"/>		
			</Signal>
		</Message>
	</Bus>
</NetworkDefinition>


